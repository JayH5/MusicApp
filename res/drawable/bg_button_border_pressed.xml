<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle" >
            <stroke
                android:color="@color/button_border_outer"
                android:width="0.5dp" />
            <corners android:radius="2dp"/>
        </shape>
    </item>
    <item
        android:top="0.5dp"
        android:bottom="0.5dp"
        android:left="0.5dp"
        android:right="0.5dp" >
        <shape android:shape="rectangle" >
            <stroke
                android:color="@color/button_border_inner"
                android:width="0.5dp" />
            <solid android:color="@android:color/white" />
            <corners android:radius="2dp"/>
        </shape>
    </item>
    <item android:drawable="@drawable/holodarkblue_list_pressed_holo_light" />

</layer-list>